<h1 class="center">Users</h1>
<table class="container striped">
    <thead>
      <tr>
          <th>ID</th>
          <th>First Name</th>
          <th>Last Name</th>
          <th>Email</th>
      </tr>
    </thead>

    <tbody>
    {{#each User}}
      <tr>
        <td>{{id}}</td>
        <td>{{firstName}}</td>
        <td>{{lastName}}</td>
        <td>{{email}}</td>
      </tr>
    {{/each}}
    </tbody>
</table>